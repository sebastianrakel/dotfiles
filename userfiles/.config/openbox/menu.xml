<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/3.4/menu">
	<menu id="apps-development-menu" label="Entwicklung">
		<item label="VirtualBox">
			<action name="Execute">
				<execute>
					VirtualBox
				</execute>
			</action>
		</item>
		<item label="MySQL Workbench">
			<action name="Execute">
				<execute>
					mysql-workbench
				</execute>
			</action>
		</item>
		<item label="NetBeans">
			<action name="Execute">
				<execute>
					netbeans
				</execute>
			</action>
		</item>
		<item label="PyCharm">
			<action name="Execute">
				<execute>
					charm
				</execute>
			</action>
		</item>
		<item label="Sublime 2">
			<action name="Execute">
				<execute>subl</execute>
			</action>
		</item>
	</menu>
	<menu id="apps-office-menu" label="Office">
		<item label="LibreOffice">
			<action name="Execute">
				<execute>
					libreoffice
				</execute>
			</action>
		</item>
	</menu>
	<menu id="apps-internet-menu" label="Internet">
		<item label="Mail">
			<action name="Execute">
				<execute>claws-mail</execute>
			</action>
		</item>
		<item label="Filezilla">
			<action name="Execute">
				<execute>
					filezilla
				</execute>
			</action>
		</item>
		<item label="Pidgin">
			<action name="Execute">
				<execute>
					pidgin
				</execute>
			</action>
		</item>
		<item label="Chromium">
			<action name="Execute">
				<execute>
					chromium
				</execute>
			</action>
		</item>
		<item label="Skype">
			<action name="Execute">
				<execute>
					skype
				</execute>
			</action>
		</item>
		<item label="Teamviewer">
			<action name="Execute">
				<execute>teamviewer</execute>
			</action>
		</item>
		<item label="JDownloader">
			<action name="Execute">
				<execute>jdownloader</execute>
			</action>
		</item>
	</menu>
	<menu id="multi-menu" label="Multimedia">
		<item label="GIMP">
			<action name="Execute">
				<execute>gimp</execute>
			</action>
		</item>
		<item label="Pavucontrol">
			<action name="Execute">
				<execute>pavucontrol</execute>
			</action>
		</item>
	</menu>
	<menu id="game-menu" label="Games">
		<item label="Minecraft">
			<action name="Execute">
				<execute>java -jar ~/Downloads/minecraft.jar</execute>
			</action>
		</item>
		<item label="Steam">
			<action name="Execute">
				<execute>steam</execute>
			</action>
		</item>
	</menu>
	<menu id="apps-fileman-menu" label="File Managers">
		<item label="SpaceFM">
			<action name="Execute">
				<execute>spacefm</execute>
			</action>
		</item>
	</menu>
	<menu id="menu-shutdown" label="Shutdown">
		<item label="Suspend">
			<action name="Execute">
				<execute>systemctl suspend</execute>
			</action>
		</item>
		<item label="Reboot">
			<action name="Execute">
				<execute>systemctl restart</execute>
			</action>
		</item>
		<item label="Shutdown">
			<action name="Execute">
				<execute>systemctl poweroff</execute>
			</action>
		</item>
	</menu>
	<menu id="system-menu" label="System">
		<item label="Openbox Configuration Manager">
			<action name="Execute">
				<command>
					obconf
				</command>
				<startupnotify>
					<enabled>
						yes
					</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="Openbox Menu">
			<action name="Execute">
				<execute>
					obmenu
				</execute>
			</action>
		</item>
		<item label="Reconfigure Openbox">
			<action name="Reconfigure"/>
		</item>
	</menu>
	<menu id="root-menu" label="Openbox 3">
		<item label="Terminal">
			<action name="Execute">
				<execute>
					roxterm
				</execute>
			</action>
		</item>
		<item label="Dateimanager">
			<action name="Execute">
				<execute>pcmanfm</execute>
			</action>
		</item>
		<item label="AusfÃ¼hren">
			<action name="Execute">
				<execute>
					dmenu_run
				</execute>
			</action>
		</item>
		<separator label="Applications"/>
		<menu id="apps-internet-menu"/>
		<menu id="apps-development-menu"/>
		<menu id="apps-office-menu"/>
		<menu id="game-menu"/>
		<menu id="multi-menu"/>
		<separator label="System"/>
		<menu id="menu-shutdown"/>
		<menu id="system-menu"/>
		<separator/>
		<item label="Log Out">
			<action name="Exit">
				<prompt>
					yes
				</prompt>
			</action>
		</item>
	</menu>
</openbox_menu>
