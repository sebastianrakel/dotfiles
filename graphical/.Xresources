! rofi settings
rofi.color-window:          #000000,#0C73C2
rofi.color-normal:          #000000,#A0A0A0,#000000,#0C73C2,#FFFFFF
rofi.color-urgent:          #000000,#FFB2B2,#000000,#0C73C2,#FFFFFF
rofi.color-active:          #000000,#C272C1,#000000,#FFFFFF,#000000
rofi.color-enabled:         true
rofi.borderwidth:           1
rofi.levenshtein-sort:      true
rofi.font: yancfont 12
rofi.padding:               3
rofi.ssh-client:            mosh
rofi.ssh-command:           termite -e '{ssh-client} {host}'
rofi.run-command:           {cmd}
rofi.run-shell-command:     termite -e '{cmd}'
rofi.lazy-filter-limit:     200
rofi.fg-active:             #FF22FF
rofi.kb-custom-1:           Alt+1
rofi.kb-custom-2:           Alt+2
rofi.kb-custom-3:           Alt+3
rofi.kb-custom-4:           Alt+4
rofi.kb-custom-5:           Alt+5
rofi.kb-custom-6:           Alt+6
rofi.kb-custom-7:           Alt+7
rofi.kb-custom-8:           Alt+8
rofi.kb-custom-9:           Alt+9

